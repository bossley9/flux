    "axios": "^1.2.6",
    "expo": "^49.0.3",
    "expo-navigation-bar": "2.3.0",
    "expo-status-bar": "1.6.0",
    "he": "^1.2.0",
    "moment": "^2.29.4",
    "node-html-parser": "^6.1.4",
    "react": "^18.2.0",
    "react-native": "^0.72.3",
